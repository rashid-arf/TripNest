{
  "name": "trip-nest",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build",
    "start": "next start",
    "prebuild": "npm run lint",
    "lint": "npm run prettier:check && npm run lint:code && npm run lint:style",
    "prettier:check": "prettier --check \"**/*.{ts,tsx,js,scss,css,jsx}\"",
    "prettier:fix": "prettier --write \"**/*.{ts,tsx,js,scss,css,jsx}\"",
    "eslint": "eslint \"./**/*.{js,jsx,ts,tsx}\"",
    "lint:code": "next lint",
    "lint:code:fix": "npm run eslint -- --fix",
    "lint:style": "stylelint \"**/*.scss\"",
    "lint:style:fix": "npm run lint:style -- --fix"
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@mui/material": "^6.5.0",
    "clsx": "^2.1.1",
    "next": "15.3.5",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "sass": "^1.89.2",
    "swiper": "^11.2.10"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "15.3.5",
    "tailwindcss": "^4",
    "typescript": "^5"
  }
}
